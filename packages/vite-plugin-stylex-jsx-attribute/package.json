{
  "name": "vite-plugin-stylex-jsx-attribute",
  "version": "1.1.0",
  "description": "Vite plugin that simplifies the use of StyleX",
  "homepage": "https://github.com/lf7817/vite-plugin-stylex-jsx-attribute/tree/main/packages/vite-plugin-stylex-jsx-attribute",
  "files": [
    "dist",
    "package.json",
    "stylex.d.ts"
  ],
